          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Get the HREF URL" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.nodes.Element;
import org.jsoup.select.Elements;
import java.net.URI;
String productIndex = vars.get(&quot;productindex&quot;);
String html = prev.getResponseDataAsString();// get response from your sampler
Document doc = Jsoup.parse(html);
Elements allLinks = doc.select(&quot;a[class=product-image]&quot;);
int index = 0;
String href = &quot;&quot;;
for(Element link : allLinks){	
	if(index.toString().equals(productIndex.toString())){
		log.info(&quot;INDEX &quot;+index.toString());
		log.info(&quot;Product INDEX &quot;+productIndex.toString());
		href = link.attr(&quot;href&quot;);
		break;
	}
	index++;
}

URI uri = new URI(href);
String domain = uri.getHost();
href=href.replace(&quot;http://&quot;,&quot;&quot;);
href=href.replace(domain,&quot;&quot;);
log.info(&quot;href--&quot;+href);
vars.put(&quot;HREF&quot;,href);</stringProp>
          </BeanShellPostProcessor>
