            <stringProp name="script">
String option_string = vars.get(&quot;SIZE_OPTIONS&quot;);
log.info(option_string);


String[] optionstring_splitted = option_string.split(&quot;,&quot;);
String size_option_id = &quot;ERROR&quot;;
for(int i = 0; i &lt; optionstring_splitted.length;i++){
	String keyValueString = optionstring_splitted[i];
	if(keyValueString.contains(&quot;id&quot;) &amp;&amp; !keyValueString.contains(&quot;outofstock&quot;)){
		keyValueString = keyValueString.replaceAll(&quot;\&quot;&quot;,&quot;&quot;);
		 keyValueString = keyValueString.replaceAll(&quot;:&quot;,&quot;&quot;);
		 keyValueString = keyValueString.replace(&quot;id&quot;,&quot;&quot;);
		 keyValueString = keyValueString.replace(&quot;{&quot;,&quot;&quot;);
		 keyValueString = keyValueString.replace(&quot;}&quot;,&quot;&quot;);	
		 size_option_id = keyValueString;
		 break; 
	}
	
}
log.info(&quot;SIZE_OPTION -&quot;+size_option_id);
vars.put(&quot;super_attr_value&quot;,size_option_id);
</stringProp>
