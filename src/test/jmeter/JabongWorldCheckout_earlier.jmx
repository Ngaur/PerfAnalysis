          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Get the Form action" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.nodes.Element;
import org.jsoup.select.Elements;
import java.net.URI;

String html = prev.getResponseDataAsString();
Document doc = Jsoup.parse(html);
Element formNode = doc.select(&quot;form[id=product_addtocart_form]&quot;).first();
String action = formNode.attr(&quot;action&quot;);

Element selectattribute = doc.select(&quot;select[class=required-entry super-attribute-select]&quot;).first();
String key_name = selectattribute.attr(&quot;name&quot;);
vars.put(&quot;super_attr&quot;,key_name);

Element formElement = doc.select(&quot;input[name=form_key]&quot;).first();
String form_key = formElement.attr(&quot;value&quot;);
vars.put(&quot;form_key&quot;,form_key);

Element productElement = doc.select(&quot;input[name=product]&quot;).first();
String product_value = productElement.attr(&quot;value&quot;);
vars.put(&quot;product_value&quot;,product_value);

Element relatedproductElement = doc.select(&quot;input[id=related-products-field]&quot;).first();
String related_product_value = relatedproductElement.attr(&quot;value&quot;);
vars.put(&quot;related_product_value&quot;,related_product_value);



URI uri = new URI(action);
String domain = uri.getHost();
action = action.replace(domain,&quot;&quot;);
action = action.replace(&quot;http://&quot;,&quot;&quot;);
log.info(&quot;ACTION-&quot;+action);
vars.put(&quot;action&quot;,action);</stringProp>
